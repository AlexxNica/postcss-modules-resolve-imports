{
  "name": "postcss-modules-resolve-imports",
  "version": "1.0.0",
  "description": "Resolves ICSS imports",
  "main": "index.js",
  "scripts": {
    "prepublish": "in-publish && npm run -s build || in-install",
    "build": "babel index.js --out-file index.js",
    "test": "npm run test:babel",
    "test:babel": "mocha --compilers js:babel-register --require test/setup.js --ui tdd test/index.js",
    "test:node": "$npm_package_scripts_test_unit",
    "test:unit": "mocha --require test/setup.js --ui tdd test/index.js --harmony_destructuring --use_strict"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sullenor/postcss-modules-resolve-imports.git"
  },
  "keywords": [
    "css-modules",
    "postcss",
    "css",
    "postcss-plugin"
  ],
  "author": "Alexey Litvinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sullenor/postcss-modules-resolve-imports/issues"
  },
  "homepage": "https://github.com/sullenor/postcss-modules-resolve-imports#readme",
  "dependencies": {
    "async": "^1.5.2",
    "lodash": "^4.6.1",
    "postcss": "^5.0.19",
    "postcss-modules-extract-exports": "^1.0.0",
    "promisify-api": "^1.1.0",
    "resolve": "^1.1.7"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "in-publish": "^2.0.0",
    "mocha": "^2.4.5",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0"
  }
}
